{
    "name": "madridianfox/laravel-prometheus",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Madridianfox\\LaravelPrometheus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Madridianfox\\LaravelPrometheus\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "laravel/framework": "^9",
        "orchestra/testbench": "^7",
        "phpunit/phpunit": "^9"
    },
    "require": {
        "php": ">=8.1",
        "ext-redis": "*",
        "promphp/prometheus_client_php": "2.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Madridianfox\\LaravelPrometheus\\PrometheusServiceProvider"
            ]
        }
    }
}
